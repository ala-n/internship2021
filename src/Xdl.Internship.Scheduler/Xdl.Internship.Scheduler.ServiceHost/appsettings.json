{
    "AllowedHosts": "*",
    "Serilog": {
        "Using": [],
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "System": "Warning"
            }
        },
        "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
        "WriteTo": [
            { "Name": "Console" },
            {
                "Name": "File",
                "Args": {
                    "path": "/Users/misakarpa/Desktop/serilog1.txt",
                    "outputTemplate": "{TimeStamp:G} {Message}{NewLine:1}{Exception:1}"
                }
            },
            {
                "Name": "File",
                "Args": {
                    "path": "/Users/misakarpa/Desktop/serilog2.json",
                    "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog"
                }
            }
        ]
    },
    "RabbitMq": {
        "Hostname": "rabbitmq",
        "QueueName": "Queue",
        "UserName": "user",
        "Password": "password"
    }
}

